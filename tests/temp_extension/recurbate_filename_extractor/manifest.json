{
  "manifest_version": 3,
  "name": "Recurbate Filename Extractor",
  "version": "1.0",
  "description": "Extracts expected video filename from recurbate video pages",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["*://*.recu.me/*/video/*/play"],
      "js": ["content-script.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "Recurbate Extractor"
  }
}